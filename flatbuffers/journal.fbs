namespace journal;

enum Namespace:uint32{
    Value=11
}

table RequestJournal{}

table JournalItem{
    last_message_timestamp:uint64;
    message_code:uint32;
    message_string:string;
    message_data:uint32;
    message_count:uint32;
}

table ResponseJournal{
    journal_items:[JournalItem];
}


union Requests{
    RequestJournal,   
}

union Responses{
    ResponseJournal,
}

table RequestWrapper{
    request:Requests;
}

table ResponseWrapper{
    response:Responses;
}
